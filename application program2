def merge_sort(arr):
    if len(arr) <= 1:
        return arr
    
    mid = len(arr) // 2
    left = arr[:mid]
    right = arr[mid:]
    
    left = merge_sort(left)
    right = merge_sort(right)
    
    return merge(left, right)

def merge(left, right):
    result = []
    i = j = 0
    
    while i < len(left) and j < len(right):
        if left[i] < right[j]:
            result.append(left[i])
            i += 1
        else:
            result.append(right[j])
            j += 1
    
    result.extend(left[i:])
    result.extend(right[j:])
    
    return result

def main():
    # Read mobile numbers from a file or accept user input
    # Assuming the mobile numbers are stored in a list called 'mobile_numbers'
    
    sorted_mobile_numbers = merge_sort(mobile_numbers)
    
    # Display the sorted mobile numbers to the user

if _name_ == "_main_":
    main()
